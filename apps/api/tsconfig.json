{
  "compilerOptions": {
    "lib": ["ES2024"],
    "target": "es2024",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "sourceMap": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*", "src/handlers/test.handler.ts"],
  "exclude": ["node_modules", "**/*.test.ts"]
}
